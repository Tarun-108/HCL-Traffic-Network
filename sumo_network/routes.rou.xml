<routes>
    <vType id="car" accel="1.0" decel="4.5" length="5" maxSpeed="25" sigma="0.5" />

    <!-- Eastbound: A0 → A1 → B1 -->
    <route id="r0" edges="A0A1 A1B1"/>

    <!-- Westbound: B1 → A1 → A0 -->
    <route id="r1" edges="B1A1 A1A0"/>

    <!-- Southbound: A0 → B0 → B1 -->
    <route id="r2" edges="A0B0 B0B1"/>

    <!-- Northbound: B1 → B0 → A0 -->
    <route id="r3" edges="B1B0 B0A0"/>

    <flow id="f0" type="car" route="r0" begin="0" end="1000" vehsPerHour="600"/>
    <flow id="f1" type="car" route="r1" begin="0" end="1000" vehsPerHour="600"/>
    <flow id="f2" type="car" route="r2" begin="0" end="1000" vehsPerHour="600"/>
    <flow id="f3" type="car" route="r3" begin="0" end="1000" vehsPerHour="600"/>
</routes>
