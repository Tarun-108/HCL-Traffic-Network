<routes>
    <vType id="car" accel="1.0" decel="4.5" length="5" maxSpeed="25" sigma="0.5"/>

    <!-- Define a few custom routes -->
    <route id="r0" edges="A0_A1 A1_A2 A2_A3"/>
    <route id="r1" edges="D3_D2 D2_D1 D1_D0"/>
    <route id="r2" edges="A0_B0 B0_C0 C0_D0"/>
    <route id="r3" edges="D3_C3 C3_B3 B3_A3"/>

    <!-- Random traffic generation -->
    <flow id="f0" type="car" begin="0" end="1000" number="100" route="r0" />
    <flow id="f1" type="car" begin="0" end="1000" number="100" route="r1" />
    <flow id="f2" type="car" begin="0" end="1000" number="100" route="r2" />
    <flow id="f3" type="car" begin="0" end="1000" number="100" route="r3" />
    
</routes>
